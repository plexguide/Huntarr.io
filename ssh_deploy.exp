#!/usr/bin/expect -f
set password "JaslenE12@@"
set user "root"
set host "10.0.0.10"
set command [lindex $argv 0]

spawn ssh $user@$host $command
expect {
    "*assword:" {
        send "$password\r"
        exp_continue
    }
    "yes/no" {
        send "yes\r"
        exp_continue
    }
}
interact
